{
  "name": "Dataspects",
  "version": "0.3.0",
  "author": ["Lex Sulzer"],
  "url": "https://www.mediawiki.org/wiki/Extension:Dataspects",
  "license-name": "GPL-2.0-or-later",
  "descriptionmsg": "dataspectssearch-desc",
  "type": "other",
  "requires": {
    "MediaWiki": ">= 1.35.0"
  },
  "AutoloadNamespaces": {
    "MediaWiki\\Extension\\Dataspects\\": "src/"
  },
  "SpecialPages": {
    "Dataspects": "SpecialDataspects"
  },
  "AutoloadClasses": {
    "SpecialDataspects": "includes/specials/SpecialDataspects.php",
    "DataspectsAPI": "includes/api/DataspectsAPI.php"
  },
  "HookHandlers": {
    "DataspectsHooks": {
      "class": "MediaWiki\\Extension\\Dataspects\\Hooks"
    }
  },
  "Hooks": {
    "PageSaveComplete": "DataspectsHooks",
    "BeforePageDisplay": "DataspectsHooks"
  },
  "MessagesDirs": {
    "Dataspects": ["i18n"]
  },
  "ResourceModules": {
    "ext.dataspectsSearch": {
      "localBasePath": "resources/ext.dataspectsSearch",
      "remoteExtPath": "Dataspects/resources/ext.dataspectsSearch",
      "dependencies": [],
      "styles": ["dataspectsSearch.css"],
      "packageFiles": [
        "./dataspectsSearch.js",
        "./searchResultMatcher.js",
        "./profiles.json",
        "./searchResult.js",
        "./searchResultClasses/element.js",
        "./searchResultClasses/mediaWiki.js",
        "./searchResultClasses/mediaWikiMetaPage.js",
        "./searchResultClasses/sMWCindyKate.js",
        "./searchResultClasses/wikiDataspects.js",
        "./searchResultClasses/searchFacet.js",
        "./searchResultClasses/code.js",
        "./instant-meilisearch.umd.js",
        "./instantsearch.production.min.js",
        "./DSneo4j.js",
        "./vis-network.min.js",
        "./helpers.js"
      ],
      "messages": []
    }
  },
  "JobClasses": {
    "dataspectsTikaJob": "MediaWiki\\Extension\\Dataspects\\DataspectsTikaJob",
    "dataspectsSpacyJob": "MediaWiki\\Extension\\Dataspects\\DataspectsSpacyJob",
    "dataspectsIndexJob": "MediaWiki\\Extension\\Dataspects\\DataspectsIndexJob"
  },
  "LogTypes": ["dataspects"],
  "LogNames": {
    "dataspects": "log-name-dataspects"
  },
  "LogHeaders": {
    "dataspects": "dataspects-header"
  },
  "LogActionsHandlers": {
    "dataspects/*": "LogFormatter"
  },
  "APIModules": {
    "dataspectsapi": "DataspectsAPI"
  },
  "manifest_version": 2
}
